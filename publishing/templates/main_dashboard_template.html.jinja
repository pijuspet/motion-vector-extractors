<div style="text-align: left; width: 100vw; max-width: 100vw; margin: 0; padding: 0;">

    <h3>Motion Vector Comparison (Frames 10-100)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>{{ mv_first }}</td>
            <td>{{ mv_latest }}</td>
        </tr>
    </table>

    <h3>VTune Hotspots (Top 30)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>{{ img_vtune_first }}</td>
            <td>{{ img_vtune_latest }}</td>
        </tr>
    </table>

    <h3>GitHub Commit</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                {% if git_commit_run1 %}
                <a href="{{ git_commit_run1 }}" target="_blank">{{ git_commit_run1 }}</a>
                {% else %}
                N/A
                {% endif %}
            </td>
            <td>
                {% if git_commit_run2 %}
                <a href="{{ git_commit_run2 }}" target="_blank">{{ git_commit_run2 }}</a>
                {% else %}
                N/A
                {% endif %}
            </td>
        </tr>
    </table>

    <h3>VTune Call Tree (Interactive)</h3>
    <h3>VTune Call Tree (Non Interactive)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>{{ calltree_first_interactive }}</td>
            <td>{{ calltree_latest_interactive }}</td>
        </tr>
    </table>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>{{ calltree_first_non_interactive }}</td>
            <td>{{ calltree_latest_non_interactive }}</td>
        </tr>
    </table>

    <h3>Detailed Table</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>{{ table_first }}</td>
            <td>{{ table_latest }}</td>
        </tr>
    </table>

    <h3>Grouped CPU Usage (All Streams)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>{{ cpu_first }}</td>
            <td>{{ cpu_latest }}</td>
        </tr>
    </table>

    <h3>Grouped Memory Usage (All Streams)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>{{ mem_first }}</td>
            <td>{{ mem_latest }}</td>
        </tr>
    </table>

    <h3>Detailed Reports</h3>
    <ul>
        {% if first_report_title %}
        <li><b>First Run:</b>
            <ac:link>
                <ri:page ri:content-title="{{ first_report_title }}" />
            </ac:link>
        </li>
        {% endif %}
        {% if latest_report_title %}
        <li><b>Latest Run:</b>
            <ac:link>
                <ri:page ri:content-title="{{ latest_report_title }}" />
            </ac:link>
        </li>
        {% endif %}
    </ul>
</div>