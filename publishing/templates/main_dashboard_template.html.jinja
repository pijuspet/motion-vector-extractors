<div style="text-align: left; width: 100vw; max-width: 100vw; margin: 0; padding: 0;">

    <h3>Motion Vector Comparison (Frames 10-100)</h3>
    <table class="mv-mini-table">
        <tr>
            {% for run in runs %}
            <th>{{ run.title }}</th>
            {% endfor %}
        </tr>
        <tr>
            {% for run in runs %}
            <td>
                {% if run.mv_comparison %}
                <pre style="background:#f4f4f4; border:1px solid #ccc; padding:10px;">{{ run.mv_comparison }}</pre>
                {% else %}
                <em>No motion vector comparison result available</em>
                {% endif %}
            </td>
            {% endfor %}
        </tr>
    </table>

    <h3>VTune Hotspots (Top 30)</h3>
    <table class="mv-mini-table">
        <tr>
            {% for run in runs %}
            <th>{{ run.title }}</th>
            {% endfor %}
        </tr>
        <tr>
            {% for run in runs %}
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{ run.vtune_hotspots }}" />
                </ac:image>
            </td>
            {% endfor %}
        </tr>
    </table>

    <h3>GitHub Commit</h3>
    <table class="mv-mini-table">
        <tr>
            {% for run in runs %}
            <th>{{ run.title }}</th>
            {% endfor %}
        </tr>
        <tr>
            {% for run in runs %}
            <td>
                {% if run.git_commit %}
                <a href="{{ run.git_commit }}" target="_blank">{{ run.git_commit }}</a>
                {% else %}
                N/A
                {% endif %}
            </td>
            {% endfor %}
        </tr>
    </table>

    <h3>VTune Call Tree (Interactive)</h3>
    <table class="mv-mini-table">
        <tr>
            {% for run in runs %}
            <th>{{ run.title }}</th>
            {% endfor %}
        </tr>
        <tr>
            {% for run in runs %}
            <td>
                {% if run.calltree_interactive %}
                <ac:structured-macro ac:name="html">
                    <ac:plain-text-body>
                        <![CDATA[{{ run.calltree_interactive.html }}]]>
                    </ac:plain-text-body>
                </ac:structured-macro>
                {% else %}
                <em>No call tree HTML available</em>
                {% endif %}
            </td>
            {% endfor %}
        </tr>
    </table>

    <h3>VTune Call Tree (Non Interactive)</h3>
    <table class="mv-mini-table">
        <tr>
            {% for run in runs %}
            <th>{{ run.title }}</th>
            {% endfor %}
        </tr>
        <tr>
            {% for run in runs %}
            <td>
                {% if run.calltree_non_interactive %}
                <pre
                    style="background:#f4f4f4; border:1px solid #ccc; padding:10px; max-height:400px; overflow-y:auto;">{{ run.calltree_non_interactive }}</pre>
                {% else %}
                <em>No call tree data available</em>
                {% endif %}
            </td>
            {% endfor %}
        </tr>
    </table>

    <h3>Detailed Table</h3>
    <table class="mv-mini-table">
        <tr>
            {% for run in runs %}
            <th>{{ run.title }}</th>
            {% endfor %}
        </tr>
        <tr>
            {% for run in runs %}
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{ run.detail_table }}" />
                </ac:image>
            </td>
            {% endfor %}
        </tr>
    </table>

    <h3>Grouped CPU Usage (All Streams)</h3>
    <table class="mv-mini-table">
        <tr>
            {% for run in runs %}
            <th>{{ run.title }}</th>
            {% endfor %}
        </tr>
        <tr>
            {% for run in runs %}
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{ run.cpu_chart }}" />
                </ac:image>
            </td>
            {% endfor %}
        </tr>
    </table>

    <h3>Grouped Memory Usage (All Streams)</h3>
    <table class="mv-mini-table">
        <tr>
            {% for run in runs %}
            <th>{{ run.title }}</th>
            {% endfor %}
        </tr>
        <tr>
            {% for run in runs %}
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{ run.memory_chart }}" />
                </ac:image>
            </td>
            {% endfor %}
        </tr>
    </table>

    <h3>Detailed Reports</h3>
    <ul>
        {% for run in runs %}
        {% if run.report_title %}
        <li><b>{{ run.title }}:</b>
            <ac:link>
                <ri:page ri:content-title="{{ run.report_title }}" />
            </ac:link>
        </li>
        {% endif %}
        {% endfor %}
    </ul>
</div>