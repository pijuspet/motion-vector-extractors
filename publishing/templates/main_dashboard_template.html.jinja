<div style="text-align: left; width: 100vw; max-width: 100vw; margin: 0; padding: 0;">

    <h3>Motion Vector Comparison (Frames 10-100)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                {% if mv_first %}
                <pre style="background:#f4f4f4; border:1px solid #ccc; padding:10px;">{{ mv_first }}</pre>
                {% else %}
                <em>No motion vector comparison result available</em>
                {% endif %}
            </td>
            <td>
                {% if mv_latest %}
                <pre style="background:#f4f4f4; border:1px solid #ccc; padding:10px;">{{ mv_latest }}</pre>
                {% else %}
                <em>No motion vector comparison result available</em>
                {% endif %}
            </td>
        </tr>
    </table>

    <h3>VTune Hotspots (Top 30)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{img_vtune_first}}" />
                </ac:image>
            </td>
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{img_vtune_latest}}" />
                </ac:image>
            </td>
        </tr>
    </table>

    <h3>GitHub Commit</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                {% if git_commit_run1 %}
                <a href="{{ git_commit_run1 }}" target="_blank">{{ git_commit_run1 }}</a>
                {% else %}
                N/A
                {% endif %}
            </td>
            <td>
                {% if git_commit_run2 %}
                <a href="{{ git_commit_run2 }}" target="_blank">{{ git_commit_run2 }}</a>
                {% else %}
                N/A
                {% endif %}
            </td>
        </tr>
    </table>

    <h3>VTune Call Tree (Interactive)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                {% if calltree_first_interactive %}
                <ac:structured-macro ac:name="html">
                    <ac:plain-text-body>
                        <![CDATA[{{ calltree_first_interactive.html }}]]>
                    </ac:plain-text-body>
                </ac:structured-macro>
                {% else %}
                <em>No call tree HTML available</em>
                {% endif %}
            </td>
            <td>
                {% if calltree_latest_interactive %}
                <ac:structured-macro ac:name="html">
                    <ac:plain-text-body>
                        <![CDATA[{{ calltree_latest_interactive.html }}]]>
                    </ac:plain-text-body>
                </ac:structured-macro>
                {% else %}
                <em>No call tree HTML available</em>
                {% endif %}
            </td>
        </tr>
    </table>
    <h3>VTune Call Tree (Non Interactive)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                {% if calltree_first_non_interactive %}
                <pre
                    style="background:#f4f4f4; border:1px solid #ccc; padding:10px; max-height:400px; overflow-y:auto;">{{ calltree_first_non_interactive }}</pre>
                {% else %}
                <em>No call tree data available</em>
                {% endif %}
            </td>
            <td>
                {% if calltree_latest_non_interactive %}
                <pre
                    style="background:#f4f4f4; border:1px solid #ccc; padding:10px; max-height:400px; overflow-y:auto;">{{ calltree_latest_non_interactive }}</pre>
                {% else %}
                <em>No call tree data available</em>
                {% endif %}
            </td>
        </tr>
    </table>

    <h3>Detailed Table</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{table_first}}" />
                </ac:image>
            </td>
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{table_latest}}" />
                </ac:image>
            </td>
        </tr>
    </table>

    <h3>Grouped CPU Usage (All Streams)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{cpu_first}}" />
                </ac:image>
            </td>
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{cpu_latest}}" />
                </ac:image>
            </td>
        </tr>
    </table>

    <h3>Grouped Memory Usage (All Streams)</h3>
    <table class="mv-mini-table">
        <tr>
            <th>First Run</th>
            <th>Latest Run</th>
        </tr>
        <tr>
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{mem_first}}" />
                </ac:image>
            </td>
            <td>
                <ac:image ac:thumbnail="true" ac:width="450">
                    <ri:attachment ri:filename="{{mem_latest}}" />
                </ac:image>
            </td>
        </tr>
    </table>

    <h3>Detailed Reports</h3>
    <ul>
        {% if first_report_title %}
        <li><b>First Run:</b>
            <ac:link>
                <ri:page ri:content-title="{{ first_report_title }}" />
            </ac:link>
        </li>
        {% endif %}
        {% if latest_report_title %}
        <li><b>Latest Run:</b>
            <ac:link>
                <ri:page ri:content-title="{{ latest_report_title }}" />
            </ac:link>
        </li>
        {% endif %}
    </ul>
</div>